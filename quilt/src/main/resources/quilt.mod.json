{
  "schema_version": 1,
  "mixin": [
    "stacked_shulker_nbt_fix.mixins.json",
    "stacked_shulker_nbt_fix-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "stacked_shulker_nbt_fix",
    "version": "${version}",
    "metadata": {
      "name": "Stacked Shulker NBT Fix",
      "description": "Fixes weird NBT data which prevents Shulker box stacking",
      "contributors": {
        "MrMelon54": "Owner"
      },
      "contact": {
        "homepage": "https://mrmelon54.com/minecraft/stacked-shulker-nbt-fix",
        "sources": "https://github.com/MrMelon54/stacked_shulker_nbt_fix",
        "issues": "https://github.com/MrMelon54/stacked_shulker_nbt_fix/issues"
      },
      "license": "GPL-3.0-only",
      "icon": "icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "com.mrmelon54.StackedShulkerNbtFix.quilt.StackedShulkerNbtFixQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.4"
      },
      {
        "id": "architectury",
        "version": ">=8.0.0"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}
